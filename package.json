    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
